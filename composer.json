{
  "name" : "rkeet/zf-doctrine-mvc",
  "description" : "Zend Framework with Doctrine MVC re-usable components",
  "keywords" : [
    "zend",
    "framework",
    "zend framework",
    "mvc",
    "zf",
    "zf3",
    "doctrine"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Robin Keet",
      "homepage": "https://github.com/rkeet"
    }
  ],
  "require" : {
    "php" : "^7.2",
    "ext-intl":"*",
    "ext-json":"*",
    "ext-gettext":"*",
    "doctrine/doctrine-orm-module" : "^1.1",
    "zendframework/zend-mvc-i18n": "^1.1"
  },
  "autoload" : {
    "psr-4" : {
      "Keet\\Mvc\\" : "src/"
    },
    "classmap" : [
      "src/Module.php"
    ]
  }
}
